<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shobhit.pooltool">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccount">
            <intent-filter>
                <action android:name="android.intent.action.CREATEACCOUNT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateGroup"
            android:parentActivityName=".ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.CREATEGROUP" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AddMember"
            android:parentActivityName=".ManageGroup">
            <intent-filter>
                <action android:name="android.intent.action.ADDMEMBER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MultipleContactSelect"
            android:parentActivityName=".GroupDetails">
            <intent-filter>
                <action android:name="android.intent.action.MUTIPLECONTACTSELECT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SettleUp"
            android:parentActivityName=".GroupDetails">
            <intent-filter>
                <action android:name="android.intent.action.MUTIPLECONTACTSELECT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Welcome"
            >
            <intent-filter>
                <action android:name="android.intent.action.WELCOME" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".GroupDetails"
            android:parentActivityName=".ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.GROUPDETAILS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MyBalanceSheet"
            android:parentActivityName=".GroupDetails">
            <intent-filter>
                <action android:name="android.intent.action.MYBALANCESHEET" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ContactHelp"
            android:parentActivityName=".ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MYBALANCESHEET" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Customise"
            android:parentActivityName=".AddExpense">
            <intent-filter>
                <action android:name="android.intent.action.MYBALANCESHEET" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ManageGroup"
            android:parentActivityName=".GroupDetails">
            <intent-filter>
                <action android:name="android.intent.action.MYBALANCESHEET" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AddExpense"
            android:parentActivityName=".GroupDetails">
            <intent-filter>
                <action android:name="android.intent.action.MYBALANCESHEET" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEWPAGEACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".ContactInviteFriends"
            android:parentActivityName=".ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.CONTACTINVITEFRIENDS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <intent-filter>
            <action android:name="ViewPagerActivity" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        <!-- Firebase Notifications -->
        <service android:name="MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <!-- ./Contact receving -->
        <service android:name=".ContactService">
        </service>

    </application>

</manifest>